# Payamak SOAP SMS Service - Environment
# Copy this file to ".env" and fill in your actual credentials

# Your Payamak username (from panel)
PAYAMAK_USERNAME=your_username_here

# Your Payamak password (from panel)
PAYAMAK_PASSWORD=your_password_here

# Sender number (from panel)
PAYAMAK_FROM=5000XXXXXXX

# WSDL URL for SOAP service (usually don't change)
PAYAMAK_WSDL=http://api.payamak-panel.com/post/Send.asmx?wsdl
